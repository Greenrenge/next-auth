{
  "compilerOptions": {
    "target": "es2019",
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "emitDeclarationOnly": true,
    "strictNullChecks": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "strict": false,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "stripInternal": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "outDir": "."
  },
  "exclude": [
    "./*.js",
    "./*.d.ts",
    "config",
    "**/__tests__"
  ]
}